<Page
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CajaBanco"
      xmlns:WinForms="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms" x:Class="CajaBanco.Reporte"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Reporte">

    <Grid>
        <WindowsFormsHost HorizontalAlignment="Center" Margin="0,57,0,0" VerticalAlignment="Top" MinWidth="1000" MinHeight="700">
            
                <WinForms:ReportViewer x:Name="ReportViewer1" Load="ReportViewer1_Load"/>
            
        </WindowsFormsHost>
        <Button x:Name="BtMenu" Content="Menu [Esc]" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="BtMenu_Click"/>

    </Grid>
</Page>
