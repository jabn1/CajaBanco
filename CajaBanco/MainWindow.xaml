<Window x:Class="CajaBanco.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CajaBanco"
        mc:Ignorable="d"
        
        Title="APLICACION DE CAJA BankINTEC" Height="450" Width="800" WindowState="Maximized" Foreground="Black">
    <Window.Background>
        <VisualBrush TileMode="Tile" Viewport="0,0,0.125,0.125">
            <VisualBrush.Visual>
                <Image UseLayoutRounding="True" d:DesignUseLayoutRounding="True" Source="backgroundCaja.png"/>
            </VisualBrush.Visual>
        </VisualBrush>
    </Window.Background>
</Window>
